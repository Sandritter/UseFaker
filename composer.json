{
  "name": "mSand/useFaker",
  "type": "library",
  "description": "test library which depends on Faker.",
  "keywords": [
    "faker",
    "fixtures",
    "data"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Michael Sandritter"
    }
  ],
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/Sandritter/Faker"
    }
  ],
  "require": {
    "php": ">=5.3.3",
    "mSand/faker": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "squizlabs/php_codesniffer": "~1.5",
    "phpunit/phpcov" : "2.0.*"
  },
  "autoload": {
    "psr-4": {
      "UseFaker\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "UseFaker\\PHPUnit\\": "test/"
    }
  }
}
